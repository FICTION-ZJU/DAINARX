{
  "automation": {
    "var": "x1, x2",
    "mode": [
      {
        "id": 1,
        "eq": "x1[1] = 3 * x1[0] - 3 * x1[0] * x2[0], x2[1] = x1[0] * x2[0] - x2[0]"
      },
      {
        "id": 2,
        "eq": "x1[1] = 2 * x1[0] - 2 * x1[0] * x2[0], x2[1] =  0.5 * x1[0] * x2[0] - 0.5 * x2[0]"
      }
    ],
    "edge": [
